{
    "name": "antigravity-storage-manager",
    "displayName": "Antigravity Storage Manager",
    "description": "Export and Import Antigravity/Cline conversation history with a single click. Backup your AI conversations to zip files and restore them anytime.",
    "version": "0.3.4",
    "publisher": "unchase",
    "author": {
        "name": "unchase"
    },
    "icon": "icon.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/unchase/antigravity-storage-manager"
    },
    "bugs": {
        "url": "https://github.com/unchase/antigravity-storage-manager/issues"
    },
    "homepage": "https://github.com/unchase/antigravity-storage-manager#readme",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#1a1a2e",
        "theme": "dark"
    },
    "keywords": [
        "antigravity",
        "cline",
        "claude",
        "ai",
        "backup",
        "export",
        "import",
        "conversation",
        "history",
        "sync",
        "google-drive"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "antigravity-storage-manager.export",
                "title": "Export Conversations",
                "icon": "$(export)",
                "category": "Antigravity Storage"
            },
            {
                "command": "antigravity-storage-manager.import",
                "title": "Import Conversations",
                "icon": "$(import)",
                "category": "Antigravity Storage"
            },
            {
                "command": "antigravity-storage-manager.rename",
                "title": "Rename Conversation",
                "icon": "$(edit)",
                "category": "Antigravity Storage"
            },
            {
                "command": "antigravity-storage-manager.syncSetup",
                "title": "Setup Google Drive Sync",
                "icon": "$(cloud)",
                "category": "Antigravity Storage"
            },
            {
                "command": "antigravity-storage-manager.syncNow",
                "title": "Sync Now",
                "icon": "$(sync)",
                "category": "Antigravity Storage"
            },
            {
                "command": "antigravity-storage-manager.syncManage",
                "title": "Manage Synced Conversations",
                "icon": "$(list-unordered)",
                "category": "Antigravity Storage"
            },
            {
                "command": "antigravity-storage-manager.syncDisconnect",
                "title": "Disconnect Google Drive Sync",
                "icon": "$(sign-out)",
                "category": "Antigravity Storage"
            }
        ],
        "submenus": [
            {
                "id": "antigravity-storage-manager.submenu",
                "label": "Antigravity Storage"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "antigravity-storage-manager.export"
                },
                {
                    "command": "antigravity-storage-manager.import"
                },
                {
                    "command": "antigravity-storage-manager.rename"
                },
                {
                    "command": "antigravity-storage-manager.syncSetup"
                },
                {
                    "command": "antigravity-storage-manager.syncNow"
                },
                {
                    "command": "antigravity-storage-manager.syncManage"
                },
                {
                    "command": "antigravity-storage-manager.syncDisconnect"
                }
            ],
            "antigravity-storage-manager.submenu": [
                {
                    "command": "antigravity-storage-manager.export",
                    "group": "1_export@1"
                },
                {
                    "command": "antigravity-storage-manager.import",
                    "group": "1_export@2"
                },
                {
                    "command": "antigravity-storage-manager.rename",
                    "group": "2_manage@1"
                },
                {
                    "command": "antigravity-storage-manager.syncSetup",
                    "group": "3_sync@1"
                },
                {
                    "command": "antigravity-storage-manager.syncNow",
                    "group": "3_sync@2"
                },
                {
                    "command": "antigravity-storage-manager.syncManage",
                    "group": "3_sync@3"
                },
                {
                    "command": "antigravity-storage-manager.syncDisconnect",
                    "group": "3_sync@4"
                }
            ],
            "view/title": [
                {
                    "command": "antigravity-storage-manager.export",
                    "when": "view == roo-cline.SidebarProvider",
                    "group": "navigation@7"
                },
                {
                    "command": "antigravity-storage-manager.import",
                    "when": "view == roo-cline.SidebarProvider",
                    "group": "navigation@8"
                },
                {
                    "command": "antigravity-storage-manager.export",
                    "when": "view == claude-dev.SidebarProvider",
                    "group": "navigation@7"
                },
                {
                    "command": "antigravity-storage-manager.import",
                    "when": "view == claude-dev.SidebarProvider",
                    "group": "navigation@8"
                },
                {
                    "submenu": "antigravity-storage-manager.submenu",
                    "when": "view == roo-cline.SidebarProvider || view == claude-dev.SidebarProvider",
                    "group": "z_commands"
                }
            ]
        },
        "configuration": {
            "title": "Antigravity Storage Manager",
            "properties": {
                "antigravity-storage-manager.sync.autoSync": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically sync conversations on startup and at regular intervals"
                },
                "antigravity-storage-manager.sync.syncInterval": {
                    "type": "number",
                    "default": 300000,
                    "description": "Auto-sync interval in milliseconds (default: 5 minutes)"
                },
                "antigravity-storage-manager.sync.showStatusBar": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show sync status in the status bar"
                },
                "antigravity-storage-manager.sync.silent": {
                    "type": "boolean",
                    "default": false,
                    "description": "Suppress success notifications for auto-sync"
                },
                "antigravity-storage-manager.sync.machineName": {
                    "type": "string",
                    "default": "",
                    "description": "Custom name for this machine (defaults to hostname if empty)"
                },
                "antigravity-storage-manager.google.clientId": {
                    "type": "string",
                    "default": "",
                    "description": "Google Cloud OAuth2 Client ID (Desktop App)"
                },
                "antigravity-storage-manager.google.clientSecret": {
                    "type": "string",
                    "default": "",
                    "description": "Google Cloud OAuth2 Client Secret"
                }
            }
        }
    },
    "scripts": {
        "compile": "node esbuild.js",
        "compile:production": "node esbuild.js --production",
        "package": "npm run compile && node node_modules/@vscode/vsce/vsce package --no-dependencies",
        "watch": "npx tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.96.0",
        "@vscode/vsce": "^3.7.1",
        "esbuild": "^0.27.2",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "@types/archiver": "^7.0.0",
        "archiver": "^7.0.1",
        "extract-zip": "^2.0.1",
        "googleapis": "^144.0.0"
    }
}