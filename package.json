{
    "name": "antigravity-storage-manager",
    "displayName": "Antigravity Storage Manager",
    "description": "Export and Import Antigravity/Cline conversation history with a single click. Backup your AI conversations to zip files and restore them anytime.",
    "version": "0.4.1",
    "publisher": "unchase",
    "author": {
        "name": "unchase"
    },
    "icon": "icon.png",
    "license": "MIT",
    "funding": {
        "type": "patreon",
        "url": "https://www.patreon.com/unchase"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/unchase/antigravity-storage-manager"
    },
    "bugs": {
        "url": "https://github.com/unchase/antigravity-storage-manager/issues"
    },
    "homepage": "https://github.com/unchase/antigravity-storage-manager#readme",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#1a1a2e",
        "theme": "dark"
    },
    "keywords": [
        "antigravity",
        "cline",
        "claude",
        "ai",
        "backup",
        "export",
        "import",
        "conversation",
        "history",
        "sync",
        "google-drive"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "antigravity-storage-manager.export",
                "title": "%antigravity.command.export%",
                "icon": "$(export)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.import",
                "title": "%antigravity.command.import%",
                "icon": "$(import)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.rename",
                "title": "%antigravity.command.rename%",
                "icon": "$(edit)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.backupAll",
                "title": "%antigravity.command.backupAll%",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.resolveConflicts",
                "title": "%antigravity.command.resolveConflicts%",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.syncSetup",
                "title": "%antigravity.command.syncSetup%",
                "icon": "$(cloud)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.syncNow",
                "title": "%antigravity.command.syncNow%",
                "icon": "$(sync)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.syncManage",
                "title": "%antigravity.command.syncManage%",
                "icon": "$(list-unordered)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.syncDisconnect",
                "title": "%antigravity.command.syncDisconnect%",
                "icon": "$(sign-out)",
                "category": "%antigravity.submenu.label%"
            },
            {
                "command": "antigravity-storage-manager.showSyncStats",
                "title": "Show Sync Statistics",
                "icon": "$(graph)",
                "category": "%antigravity.submenu.label%"
            }
        ],
        "submenus": [
            {
                "id": "antigravity-storage-manager.submenu",
                "label": "%antigravity.submenu.label%"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "antigravity-storage-manager.export"
                },
                {
                    "command": "antigravity-storage-manager.import"
                },
                {
                    "command": "antigravity-storage-manager.rename"
                },
                {
                    "command": "antigravity-storage-manager.syncSetup"
                },
                {
                    "command": "antigravity-storage-manager.syncNow"
                },
                {
                    "command": "antigravity-storage-manager.syncManage"
                },
                {
                    "command": "antigravity-storage-manager.syncDisconnect"
                },
                {
                    "command": "antigravity-storage-manager.showSyncStats"
                }
            ],
            "antigravity-storage-manager.submenu": [
                {
                    "command": "antigravity-storage-manager.export",
                    "group": "1_export@1"
                },
                {
                    "command": "antigravity-storage-manager.import",
                    "group": "1_export@2"
                },
                {
                    "command": "antigravity-storage-manager.rename",
                    "group": "2_manage@1"
                },
                {
                    "command": "antigravity-storage-manager.syncSetup",
                    "group": "3_sync@1"
                },
                {
                    "command": "antigravity-storage-manager.syncNow",
                    "group": "3_sync@2"
                },
                {
                    "command": "antigravity-storage-manager.syncManage",
                    "group": "3_sync@3"
                },
                {
                    "command": "antigravity-storage-manager.syncDisconnect",
                    "group": "3_sync@4"
                }
            ],
            "view/title": [
                {
                    "command": "antigravity-storage-manager.export",
                    "when": "view == roo-cline.SidebarProvider",
                    "group": "navigation@7"
                },
                {
                    "command": "antigravity-storage-manager.import",
                    "when": "view == roo-cline.SidebarProvider",
                    "group": "navigation@8"
                },
                {
                    "command": "antigravity-storage-manager.export",
                    "when": "view == claude-dev.SidebarProvider",
                    "group": "navigation@7"
                },
                {
                    "command": "antigravity-storage-manager.import",
                    "when": "view == claude-dev.SidebarProvider",
                    "group": "navigation@8"
                },
                {
                    "submenu": "antigravity-storage-manager.submenu",
                    "when": "view == roo-cline.SidebarProvider || view == claude-dev.SidebarProvider",
                    "group": "z_commands"
                }
            ]
        },
        "configuration": {
            "title": "%antigravity.config.title%",
            "properties": {
                "antigravity-storage-manager.sync.autoSync": {
                    "type": "boolean",
                    "default": true,
                    "description": "%antigravity.config.autoSync%"
                },
                "antigravity-storage-manager.sync.syncInterval": {
                    "type": "number",
                    "default": 300000,
                    "description": "%antigravity.config.syncInterval%"
                },
                "antigravity-storage-manager.sync.showStatusBar": {
                    "type": "boolean",
                    "default": true,
                    "description": "%antigravity.config.showStatusBar%"
                },
                "antigravity-storage-manager.sync.silent": {
                    "type": "boolean",
                    "default": false,
                    "description": "%antigravity.config.silent%"
                },
                "antigravity-storage-manager.sync.machineName": {
                    "type": "string",
                    "default": "",
                    "description": "%antigravity.config.machineName%"
                },
                "antigravity-storage-manager.google.clientId": {
                    "type": "string",
                    "default": "",
                    "description": "%antigravity.config.clientId%"
                },
                "antigravity-storage-manager.google.clientSecret": {
                    "type": "string",
                    "default": "",
                    "description": "%antigravity.config.clientSecret%"
                },
                "antigravity-storage-manager.backup.enabled": {
                    "type": "boolean",
                    "default": false,
                    "description": "%antigravity.config.backupEnabled%"
                },
                "antigravity-storage-manager.backup.interval": {
                    "type": "number",
                    "default": 24,
                    "description": "%antigravity.config.backupInterval%"
                },
                "antigravity-storage-manager.backup.path": {
                    "type": "string",
                    "default": "",
                    "description": "%antigravity.config.backupPath%"
                },
                "antigravity-storage-manager.backup.retention": {
                    "type": "number",
                    "default": 10,
                    "description": "%antigravity.config.backupRetention%"
                }
            }
        }
    },
    "scripts": {
        "compile": "node esbuild.js",
        "compile:production": "node esbuild.js --production",
        "package": "npm run compile && node node_modules/@vscode/vsce/vsce package --no-dependencies",
        "watch": "npx tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "test:unit": "jest"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.96.0",
        "@vscode/vsce": "^3.7.1",
        "esbuild": "^0.27.2",
        "jest": "^30.2.0",
        "jest-mock-vscode": "^4.10.0",
        "ts-jest": "^29.4.6",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "@types/archiver": "^7.0.0",
        "archiver": "^7.0.1",
        "extract-zip": "^2.0.1",
        "googleapis": "^144.0.0"
    },
    "optionalDependencies": {
        "@esbuild/win32-x64": "^0.27.2"
    }
}
